<!DOCTYPE html>
<html>
<head>
  <title>Osmanager</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<header>
		<% flash.each do |key , msg| %>
			<div class="flash <%= key %>" id="flash">
				<%= msg %>
			</div>
		<% end %>
	</header>
	<nav>
		<ul>
			<li> <%= link_to "Menu" , "#" %> </li>
			<li> <%= link_to "Clientes" , {:controller => "cliente", :action => "index"} %> </li>
			<li> <%= link_to "Contratos" , {:controller => "contratos", :action => "index"} %> </li>
			<li> <%= link_to "ServiÃ§os" , "#" %> </li>
			<li> <%= link_to "Chamados" , "#" %> </li>
			<li> <%= link_to "Produtos" , "#" %> </li>
		</ul>
	</nav>

<%= yield %>

</body>
</html>
